{
  "name": "@mobilc/panorama-react-dom",
  "version": "0.2.9",
  "description": "React for Valve's Panorama UI (for Dota 2 Custom Games).",
  "keywords": [
    "dota",
    "panorama",
    "react",
    "jsx"
  ],
  "repository": "https://github.com/takegine/testdodo/issues",
  "license": "MIT",
  "authors": [
    "ark120202",
    "Demon673",
    "西索酱"
  ],
  "exports": {
    "development": "./dist/esm/panorama-react-dom.development.js",
    "production": "./dist/esm/panorama-react-dom.production.js",
    "default": "./dist/esm/panorama-react-dom.development.js"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*.{js,ts}"
  ],
  "scripts": {
    "build": "rollup --config --environment BUILD_ENV:production",
    "dev": "rollup --config --environment BUILD_ENV:development --watch",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "send": "pnpm -r exec npm version patch | pnpm publish | git push origin master",
    "prepublishOnly": "pnpm build"
  },
  "prettier": {
    "printWidth": 100,
    "proseWrap": "always",
    "singleQuote": true,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "parserOptions": {
      "project": "tsconfig.json"
    },
    "extends": "@ark120202/eslint-config/node",
    "rules": {
      "no-restricted-syntax": "off",
      "prefer-arrow/prefer-arrow-functions": "off",
      "node/no-unsupported-features/node-builtins": "off"
    },
    "ignorePatterns": "/dist"
  },
  "dependencies": {
    "panorama-polyfill": "^0.1.1"
  },
  "peerDependencies": {
    "react": "^16.13.1 || ^17.0.0 || ^18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
