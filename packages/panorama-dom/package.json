{
  "name": "@mobilc/panorama-react-dom",
  "version": "0.3.3",
  "description": "React for Valve's Panorama UI (for Dota 2 Custom Games).",
  "keywords": [
    "dota",
    "panorama",
    "react",
    "jsx"
  ],
  "repository": "https://github.com/takegine/testdodo/issues",
  "license": "MIT",
  "authors": [
    "ark120202",
    "Demon673",
    "西索酱"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "rollup --config --environment BUILD_ENV:production",
    "dev": "tsup --watch",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@mobilc/panorama-polyfill": "workspace:^"
  },
  "peerDependencies": {
    "react": "^16.13.1 || ^17.0.0 || ^18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "minify": false,
    "splitting": false,
    "clean": true,
    "dts": true,
    "external": [
      "React",
      "@mobilc/panorama-polyfill"
    ]
  }
}
